    "webpack-split-by-path": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.1",
    "jest-cli": "^19.0.2",
