  },
  "scripts": {
    "watch": "webpack --hide-modules --config webpack.dev.config.js",
    "build": "webpack --hide-modules",
    "serve": "node server.js",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": [
      "**/tests/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "modulePaths": [
      "src"
    ],
    "collectCoverageFrom": [
      "src/*/**/*.js"
    ]
  }
