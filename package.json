  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.22.0",
    "lodash": "^4.15.0",
    "prop-types": "^15.5.7",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
